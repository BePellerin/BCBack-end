{% extends 'base.html.twig' %}

{% block title %}History{% endblock %}

{% block body %}<section class="container">

    <h1>History</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ history.id }}</td>
            </tr>
            <tr>
                <th>SellerName</th>
                <td>{{ history.sellerName }}</td>
            </tr>
            <tr>
                <th>BuyerName</th>
                <td>{{ history.buyerName }}</td>
            </tr>
            <tr>
                <th>UpdateAt</th>
                <td>{{ history.updateAt ? history.updateAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_history_index') }}">back to list</a>

    <a href="{{ path('app_history_edit', {'id': history.id}) }}">edit</a>

{{ include('history/_delete_form.html.twig') }}</section>

{% endblock %}
