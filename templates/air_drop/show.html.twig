        </tbody>
    </table>

    <a href="{{ path('app_air_drop_index') }}">back to list</a>

    <a href="{{ path('app_air_drop_edit', {'id': air_drop.id}) }}">edit</a>

{{ include('air_drop/_delete_form.html.twig') }}</section>

